<!-- Tiedoston nimi on users.ejs -->
<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Tuodaan HEAD-tägin sisälle tiedoston /partials/head -sisältö -->
    <%- include('../partials/head') %>
</head>

<body>

    <header>
        <!-- Tuodaan HEADER-tägin sisälle tiedoston /partials/header -sisältö -->
        <%- include('../partials/header_kattisivu') %>
    </header>
    <table>
        <% kattila.forEach(function(katti){ %>
            <% if (katti.name !="John" ) { %>
                <tr>
                    <td>
                        <%= katti.name %>
                    </td>
                    <td>
                        <%= katti.date %>
                    </td>
                </tr>
                <% } %>
                    <% })%>
    </table>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN"
        crossorigin="anonymous"></script>

</body>

</html>